apiVersion: mysql.sql.crossplane.io/v1alpha1
kind: ProviderConfig
metadata:
  name: sql-config
spec:
  credentials:
    source: Secret
    secretRef:
      name: demo-mysql-conn
      namespace: demo
      key: MYSQL_PASSWORD
  endpointRef:
    name: demo-db-endpoint
    key: host # The key kro used for the IP/Host
  user: admin